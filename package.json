{
  "name": "psqlBuddy",
  "version": "0.1.10",
  "author": "Hamilton Junior",
  "repository": {
    "type": "git",
    "url": "https://github.com/Hamilton-Junior/psqlBuddy"
  },
  "type": "module",
  "main": "main.js",
  "scripts": {
    "dev": "vite --host 127.0.0.1",
    "server": "node server.js",
    "electron": "electron .",
    "start": "npm run dev:all",
    "dev:all": "concurrently -n \"VITE,SERVER,APP\" -c \"blue,green,magenta\" \"npm run dev\" \"npm run server\" \"cross-env SKIP_BACKEND=1 npm run electron\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "dist": "npm run build && electron-builder"
  },
  "dependencies": {
    "@google/genai": "1.35.0",
    "@monaco-editor/react": "4.7.0",
    "cors": "2.8.5",
    "express": "^5.2.1",
    "html2canvas": "1.4.1",
    "jspdf": "^4.0.0",
    "lucide-react": "0.562.0",
    "pg": "8.16.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hot-toast": "2.6.0",
    "recharts": "3.6.0"
  },
  "devDependencies": {
    "@types/react": "19.2.8",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.2",
    "autoprefixer": "10.4.23",
    "concurrently": "9.2.1",
    "cross-env": "10.1.0",
    "electron": "^39.2.7",
    "electron-builder": "26.4.0",
    "postcss": "8.5.6",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3",
    "vite": "^7.3.1"
  },
  "build": {
    "appId": "com.psqlbuddy.app",
    "productName": "PSQL Buddy",
    "copyright": "Copyright Â© 2026",
    "files": [
      "dist/**/*",
      "main.js",
      "server.js",
      "package.json",
      "node_modules/**/*"
    ],
    "directories": {
      "output": "release"
    },
    "asar": true,
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
